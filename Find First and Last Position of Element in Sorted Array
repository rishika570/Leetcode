class Solution {
    public int[] searchRange(int[] nums, int target) {
        int a[]=new int[2];
        int sindex=-1,lindex=-1;
        for(int i=0;i<nums.length;i++){
      if(nums[i]==target){
      sindex=i;
          break;
    }}
        for(int i=nums.length-1;i>=0;i--){
    if(nums[i]==target){
lindex=i;
    break;}}
        
    a[0]=sindex;
    a[1]=lindex;    
        return a;
    }
}
